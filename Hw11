
--Q1
(SELECT first_name FROM actor)
union
(SELECT first_name FROM customer)


--Q2
(SELECT first_name FROM actor)
intersect
(SELECT first_name FROM customer)


--Q3
(SELECT first_name FROM actor)
except
(SELECT first_name FROM customer)


--Q4

(SELECT first_name FROM actor)
union all
(SELECT first_name FROM customer)

(SELECT first_name FROM actor)
intersect all
(SELECT first_name FROM customer)

(SELECT first_name FROM actor)
except all
(SELECT first_name FROM customer)
